<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<runtime>
   <time>
      <frequency value="0"/>
   </time>
   <states>
      <state name="NA_Node_Overlay"/>
      <state name="NA_Flow_Forward"/>
      <state name="NA_Flow_Reverse"/>
   </states>
   <types>
      <object name="na_tool_link_node">
         <NACanvasObject/>
      </object>
      <object name="na_link_attribute">
         <NACanvasObject/>
         <NACanvasObjectBasic>
            <items>
               <group name="overlay">
                  <image file="NA_ResultsOverlayLinks.svg">
                     <translate x="0" y="-38"/>
                  </image>
                  <text name="number">
                     <textColor>
                        <string value="white"/>
                     </textColor>
                     <enableBackground value="false"/>
                     <translate x="20" y="-20"/>
                     <scale x="2.0" y="2.0"/>
                  </text>
               </group>
            </items>
            <text>
               <item name="number" attribute="NA_Node_Rank"/>
            </text>
            <switch>
               <state name="NA_Node_Overlay" group="overlay">
                  <item name="overlay"/>
               </state>
            </switch>
         </NACanvasObjectBasic>
      </object>
      <object name="na_node">
         <NACanvasObject/>
         <NACanvasObjectBasic>
            <items>
               <image name="node" file="NA_Node.svg">
                  <translate x="-10" y="-10"/>
               </image>
               <group name="overlay">
                  <image file="NA_ResultsOverlay.svg">
                     <translate x="-19.5" y="-37.5"/>
                  </image>
                  <text name="number">
                     <textColor>
                        <string value="white"/>
                     </textColor>
                     <enableBackground value="false"/>
                     <translate x="20" y="-20"/>
                     <scale x="2.0" y="2.0"/>
                  </text>
               </group>
               <text name="name">
                  <textColor>
                     <string value="white"/>
                     <alpha value="0.75"/>
                  </textColor>
                  <outlineColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </outlineColor>
                  <backgroundColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </backgroundColor>
                  <translate x="50" y="0"/>
               </text>
            </items>
            <text>
               <item name="number" attribute="NA_Node_Rank"/>
               <item name="name" attribute="NA_Node_Name"/>
            </text>
            <switch>
               <state>
                  <item name="node"/>
                  <item name="name"/>
               </state>
               <state name="NA_Node_Overlay" group="overlay">
                  <item name="overlay"/>
               </state>
               <!-- <flag attribute="NA_Node_Overlay">
                  <item name="overlay"/>
               </flag> -->
            </switch>
         </NACanvasObjectBasic>
      </object>
   </types>
</runtime>
</dmz>
